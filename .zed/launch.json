{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: Run Server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/manage.py",
      "args": ["runserver", "127.0.0.1:8000"],
      "django": true,
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "DJANGO_SETTINGS_MODULE": "PharmacyOnDuty.settings"
      }
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
